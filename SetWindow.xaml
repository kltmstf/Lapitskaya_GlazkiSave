        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lapitskaya_GlazkiSave"
        mc:Ignorable="d"
        Title="SetWindow" Height="200" Width="300" >
